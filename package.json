{
  "name": "fm-mini",
  "version": "1.0.0",
  "description": "WebLLM Chat with Nodes List",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:server": "nodemon server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:tensor": "node tests/tensor_parallel/run-tests.js",
    "test:simple": "node tests/tensor_parallel/run-tests.js --simple",
    "test:direct": "node tests/tensor_parallel/direct-test.js",
    "download:models": "node tests/tensor_parallel/model-manager.js",
    "download:tiny-model": "node tests/tensor_parallel/tiny-model-downloader.js",
    "create:mock-model": "node tests/tensor_parallel/create-mock-model.js"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.78",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.31",
    "puppeteer": "^24.4.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.3.5",
    "uuid": "^11.1.0",
    "vite": "^5.0.0"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".mjs"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
